<li class="tab-panel" data-mobile-target="fit-tips">
  {%- for block in section.blocks -%}
    <div class="accordion">
      <h3 class="block"><span class="plus">+</span>{{ block.settings.question }}</h3>
      <p class="pane">{{ block.settings.pane }}</p>
    </div>
  {%- endfor -%}
</li>

{% schema %}
  {
    "name": "Fit Guide",
    "max_blocks": 30,
    "blocks": [
      {
        "type": "fit-guide-block",
        "name": "Fit Guide Block",
        "settings": [
          {
            "type": "text",
            "id": "question",
            "label": "Question"
          },
          {
            "type": "textarea",
            "id": "pane",
            "label": "Pane"
          }
        ]
      }
    ]
  }
{% endschema %}
