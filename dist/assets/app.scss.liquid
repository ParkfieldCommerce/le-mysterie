/* SVG Icons =================================================================*/

// Don't forget to compress your icons! https://jakearchibald.github.io/svgomg/
// All icons must be created at 32px by 32px regardless of size to be scaled to

.icon {
  vertical-align: middle;
  display: inline-block;
  line-height: 1%; // Makes inline SVGs behave more like text characters
  svg {
    overflow: visible;
    fill: none;
    stroke: none;
    .fill {
      fill: $colorBrand;
    }
    .stroke {
      stroke: $colorBrand;
      stroke-width: $borderWeight;
      stroke-linecap: square;
      stroke-miterlimit: 10;
    }
  }
}

$button-size: 25px;
$button-color: #000;

@function calc-line-height($n) {
  @return $n / 5;
}

@mixin css3-rotation($n){
  -webkit-transform:rotate($n);
  transform: rotate($n);
}

.hamburguer{
  width: $button-size;
  height: $button-size;
  display: block;
  position: relative;
  background: none;
  margin: 0 0 0px 0;

  &:after, &:before, span, label{
    content: ' ';
    display: block;
    width: 100%;
    height: 2px;
    background: $button-color;
    left:0;
    position: absolute;
    -webkit-transition: all 300ms ease-in-out;
    transition: all 300ms ease-in-out;
  }
  &:before{
    top: 1px;
  }
  &:after{
    bottom: 4px;
  }
  &:focus{
    outline: none;
  }
  label{
    width: 100%;
    height: 100%;
    background: none;
    cursor: pointer;
    z-index: 1000;
  }
  span{
    top: calc-line-height( $button-size ) * 2;
  }
}

#change-hamburguer{
  display:none;

  &:checked ~ .hamburguer:before, &:checked ~ .hamburguer:after {
    top: $button-size / 2;
    margin-top: -10%;
  }
  &:checked ~ .hamburguer:before{
		@include css3-rotation(-45deg);
  }
  &:checked ~ .hamburguer:after{
		@include css3-rotation(45deg);
  }
  &:checked ~ .hamburguer span{
		opacity: 0;
  }
  &:checked ~ .menu{
    max-height: 200px;
  }
}

