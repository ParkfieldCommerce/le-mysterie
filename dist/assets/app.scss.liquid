/* Popups ====================================================================*/

/// Basic popup styling
.popup {
  position: fixed;
  z-index: 9996;
  opacity: 0;
  visibility: hidden;
  @include transition();
  &.visible {
    opacity: 1;
    visibility: visible;
    @include transition();
  }
  .popup-content {
    display: inline-block;
    z-index: 9999;
    max-width: 100%;
    max-height: 100%;
    overflow: auto; // Optional line for overly tall popups
    padding: $gutter;
    background-color: $colorBackground;
    text-align: initial;
    cursor: default;
    white-space: initial;
    color: $colorText;
  }
  &.overlay {
    background: rgba($colorText, 0.85);
    @include overlay();
    .popup-outside {
      position: absolute;
      z-index: 9998;
      @include overlay();
    }
    .popup-inner {
      -webkit-backface-visibility: hidden;
      cursor: pointer;
      z-index: 9997;
      text-align: center;
      white-space: nowrap;
      @include overlay();
      @include prefixer(transition-delay, 0.3s, $prefixes);
      @include prefixer(transform, translateY(-$gutter/2), $prefixes);
      @include transition();
    }
    .popup-content {
      @include vertical-align();
    }
    &.visible {
      .popup-inner {
        @include prefixer(transform, translateY(0px), $prefixes);
      }
    }
  }
  &.notification {
    .popup-content {
      position: fixed;
      bottom: $gutter;
      left: $gutter;
      padding: $gutter/2;
      @include prefixer(transition-delay, 0.3s, $prefixes);
      @include prefixer(transform, translateY(-$gutter/2), $prefixes);
      @include transition();
    }
    &.visible {
      .popup-content {
        @include prefixer(transform, translateY(0px), $prefixes);
      }
    }
  }
  .popup-close {
    display: block;
    position: absolute;
    top: $gutter/2;
    right: $gutter/2;
  }
}

