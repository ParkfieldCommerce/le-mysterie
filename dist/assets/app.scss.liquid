/* Mixins ====================================================================*/
// Prefixer (See https://github.com/thoughtbot/bourbon/blob/master/app/assets/stylesheets/addons/_prefixer.scss)
@mixin prefixer($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else
    if $prefix == moz {
      -moz-#{$property}: $value;
    } @else
    if $prefix == ms {
      -ms-#{$property}: $value;
    } @else
    if $prefix == o {
      -o-#{$property}: $value;
    } @else
    if $prefix == spec {
      #{$property}: $value;
    } @else {
      @warn "Unrecognized prefix: #{$prefix}";
    }
  }
}
// Clearfix
@mixin clearfix() {
  display: block; // TBC - is this OK? Seems to help

  &:after {
    content: '';
    display: table;
    clear: both;
  }
  *zoom: 1;
}
// Screen Sizes
@mixin s() {
  @media (max-width: $s) {
    @content;
  }
}
@mixin s-m() {
  @media (max-width: $l - 1px) {
    @content;
  }
}
@mixin m() {
  @media (min-width: $s + 1px) and (max-width: $l - 1px) {
    @content;
  }
}
@mixin m-l() {
  @media (min-width: $s + 1px) and (max-width: $l) {
    @content;
  }
}
@mixin l() {
  @media (min-width: $l) {
    @content;
  }
}
@mixin m-xl() {
  @media (min-width: $s + 1px) {
    @content;
  }
}
